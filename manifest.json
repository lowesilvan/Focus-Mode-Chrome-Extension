{
    "manifest_version": 2,
    "name": "Focus Mode",
    "version": "0.0.1",
    "description": "Block most distracting websites when working so you don't get easily derailed by a simple notification. Focus Mode",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ], 
    "icons": {
        "16": "favicon.ico",
        "48": "favicon.ico",
        "128": "favicon.ico"
    },

    "browser_action": {
        "default_popup": "Popup.html",
        "default_title": "Focus Mode"
    }
}